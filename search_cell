set search_target ts07nxpllogl08hdl057f_frame_timing_ccs:HDBLVT08_FSDPRBQOM8_DY4FS_1.frame
set list_cells [collection_to_list -name_only [get_cells *]]

foreach cell [lindex $list_cells 0] {
	report_attributes -application [get_cells $cell] > cell_attributes.log
	set ref_phys_block [exec grep "ref_phys_block" cell_attributes.log | awk {{print $5}}]
	if {![string compare $ref_phys_block $search_target]} {
		exec echo "find: $cell"
		break
	}
}
